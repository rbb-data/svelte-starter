{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "checkJs": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "paths": {
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"],
      "$data": ["src/data"],
      "$data/*": ["src/data/*"],
      "$shared": ["src/lib/components/shared"],
      "$shared/*": ["src/lib/components/shared/*"]
    }
  }
}
