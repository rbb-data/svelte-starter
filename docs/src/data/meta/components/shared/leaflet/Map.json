{
  "name": "Map",
  "description": "Renders a Leaflet map",
  "props": [
    {
      "name": "height",
      "description": "height of the map",
      "type": "number",
      "optional": false,
      "default": null,
      "isExposed": false
    },
    {
      "name": "options",
      "description": "map options (see https://leafletjs.com/reference.html#map)",
      "type": "MapOptions",
      "optional": true,
      "default": "undefined",
      "isExposed": false
    },
    {
      "name": "fitBounds",
      "description": "if given, the map will zoom in on the given bounds",
      "type": "LatLngBoundsLiteral",
      "optional": true,
      "default": "undefined",
      "isExposed": false
    }
  ],
  "slots": [
    "default"
  ]
}