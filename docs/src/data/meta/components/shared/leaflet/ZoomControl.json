{
  "name": "ZoomControl",
  "description": "Renderless component that adds a zoom control to a Leaflet map",
  "props": [
    {
      "name": "position",
      "description": "position of the zoom control",
      "type": "\"topleft\" | \"topright\" | \"bottomleft\" | \"bottomright\"",
      "optional": true,
      "default": "'topright'",
      "isExposed": false
    },
    {
      "name": "zoomInText",
      "description": "zoom in icon (any valid html string is allowed)",
      "type": "string",
      "optional": true,
      "default": "'+'",
      "isExposed": false
    },
    {
      "name": "zoomInTitle",
      "description": "zoom in title for accessibility",
      "type": "string",
      "optional": true,
      "default": "'Hineinzoomen'",
      "isExposed": false
    },
    {
      "name": "zoomOutText",
      "description": "zoom out icon (any valid html string is allowed)",
      "type": "string",
      "optional": true,
      "default": "'&#x2212'",
      "isExposed": false
    },
    {
      "name": "zoomOutTitle",
      "description": "zoom out title for accessibility",
      "type": "string",
      "optional": true,
      "default": "'Herauszommen'",
      "isExposed": false
    }
  ]
}