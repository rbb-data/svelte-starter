{
  "name": "LocalMap",
  "description": "Bing-powered Leaflet map of Berlin or Brandenburg",
  "props": [
    {
      "name": "height",
      "description": "height of the map",
      "type": "number",
      "optional": false,
      "default": null,
      "isExposed": false
    },
    {
      "name": "location",
      "description": "region to show",
      "type": "\"berlin\" | \"brandenburg\"",
      "optional": false,
      "default": null,
      "isExposed": false
    },
    {
      "name": "showDistricts",
      "description": "if true, show Berlin's Bezirke or Brandenburg's Kreise",
      "type": "boolean",
      "optional": true,
      "default": "false",
      "isExposed": false
    },
    {
      "name": "styleSheet",
      "description": "if given, styles are passed to the Bing layer",
      "type": "Record<string, string>",
      "optional": true,
      "default": "undefined",
      "isExposed": false
    },
    {
      "name": "maskOptions",
      "description": "map options for the mask (see https://leafletjs.com/examples/geojson/)",
      "type": "GeoJSONOptions<any>",
      "optional": true,
      "default": "undefined",
      "isExposed": false
    },
    {
      "name": "districtOptions",
      "description": "map options for districts (see https://leafletjs.com/examples/geojson/)",
      "type": "GeoJSONOptions<any>",
      "optional": true,
      "default": "undefined",
      "isExposed": false
    }
  ],
  "slots": [
    "default"
  ]
}