{
  "name": "@rbb-data/svelte-starter",
  "version": "0.1.5",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "postinstall": "scripts/setup.sh",
    "dev": "svelte-kit dev",
    "package": "svelte-kit package",
    "build": "svelte-kit build",
    "build:rbb-online": "BASE_PATH=/static/rbb/rbb-data/{project-name} BUILD_DIR={project-name} npm run build",
    "build:google-cloud": "BASE_PATH=/rbb-data-static/{project-name} npm run build",
    "preview": "svelte-kit preview",
    "upload": "rsync -srvP --rsync-path='sudo rsync' -e 'ssh -i ~/.ssh/rbb-data.pem' build/ linux@rbb-dj-dev.app.rbb-cloud.de:/var/www/{project-name}/",
    "deploy": "npm run build && npm run upload",
    "predeploy:google-cloud": "gcloud config set project rbb-data-static-file-server",
    "deploy:google-cloud": "npm run build:google-cloud && gsutil -m rsync -rd build/ gs://rbb-data-static/{project-name}",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "update-tokens": "node style-dictionary/build.cjs && npx prettier --ignore-path .gitignore --write --plugin-search-dir=. ./src/**/tokens.{css,js}",
    "update-google-doc": "node scripts/update-google-doc.js --preserve-styles && npx prettier --ignore-path .gitignore --write --plugin-search-dir=. ./src/data/google-doc.json",
    "update-google-sheets": "node scripts/update-google-sheets.js"
  },
  "dependencies": {
    "d3-array": "^3.1.1",
    "d3-scale": "^4.0.2",
    "fuzzysort": "^1.1.4",
    "leaflet": "^1.7.1",
    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "@rollup/plugin-dsv": "^2.0.3",
    "@sveltejs/adapter-static": "1.0.0-next.32",
    "@sveltejs/kit": "1.0.0-next.345",
    "@types/d3-array": "^3.0.2",
    "@types/d3-dsv": "^3.0.0",
    "@types/d3-scale": "^4.0.2",
    "@types/geojson": "^7946.0.8",
    "@types/leaflet": "^1.7.6",
    "@types/sanitize-html": "^2.5.0",
    "archieml": "^0.4.2",
    "d3-dsv": "^3.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "googleapis": "^100.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-jsdoc": "^0.3.30",
    "prettier-plugin-svelte": "^2.5.0",
    "sanitize-html": "^2.5.3",
    "style-dictionary": "^3.0.3",
    "svelte": "^3.42.6",
    "svelte2tsx": "^0.4.10",
    "typescript": "^4.5.2",
    "yaml": "^1.10.2"
  },
  "overrides": {
    "node-forge": "^1.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=14.13"
  }
}
