{
  "name": "@rbb-data/svelte-starter",
  "version": "0.0.1",
  "private": "true",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "deploy": "npm run build && scripts/deploy-to-gc.sh {project-name}",
    "deploy--experimental": "BASE_PATH=/rbb-data-static/{project-name}-experimental npm run build && scripts/deploy-to-gc.sh {project-name}-experimental",
    "update:gdoc": "node scripts/update-google-doc.js --preserve-styles && npx prettier --ignore-path .gitignore --write --plugin-search-dir=. ./src/data/google-doc.json",
    "update:gsheet": "node scripts/update-google-sheets.js",
    "update:tokens": "node style-dictionary/build.cjs && npx prettier --ignore-path .gitignore --write --plugin-search-dir=. ./src/**/tokens.{css,scss,js}",
    "update:all": "npm run update:gdoc; npm run update:gsheet; npm run update:tokens",
    "docs": "start-storybook --docs -p 6006 -s ./static --no-open",
    "docs:build": "build-storybook --docs -o docs-build -s ./static",
    "docs:deploy": "scripts/deploy-docs.sh",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch"
  },
  "dependencies": {
    "colorjs.io": "^0.3.0",
    "d3-array": "^3.1.1",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.1.0",
    "focus-visible": "^5.2.0",
    "fuzzysort": "^1.1.4",
    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@rollup/plugin-dsv": "^2.0.3",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-svelte-csf": "^2.0.4",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/svelte": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/adapter-static": "1.0.0-next.39",
    "@sveltejs/kit": "1.0.0-next.405",
    "@types/d3-array": "^3.0.2",
    "@types/d3-dsv": "^3.0.0",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-shape": "^3.1.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/sanitize-html": "^2.5.0",
    "archieml": "^0.4.2",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.5",
    "d3-dsv": "^3.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "googleapis": "^100.0.0",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.4.1",
    "prettier-plugin-jsdoc": "^0.3.30",
    "prettier-plugin-svelte": "^2.5.0",
    "sanitize-html": "^2.5.3",
    "sass": "^1.49.11",
    "style-dictionary": "^3.0.3",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.5",
    "svelte2tsx": "^0.4.10",
    "vite": "^3.1.4"
  },
  "overrides": {
    "node-forge": "^1.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=14.13"
  }
}
