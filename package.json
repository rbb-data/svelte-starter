{
  "name": "@rbb-data/svelte-starter",
  "version": "0.0.1",
  "private": "true",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "deploy": "scripts/deploy-to-gcs.sh --build",
    "deploy--experimental": "export $(cat .env | xargs) && scripts/deploy-to-gcs.sh --build /rbb-data-static/${PROJECT_CREATION_DATE}-${PROJECT_NAME}-experimental",
    "update:gdoc": "node scripts/update-google-doc.js --preserve-styles && npx prettier --write --plugin-search-dir=. ./src/data/google-doc.json",
    "update:gsheet": "node scripts/update-google-sheets.js",
    "update:tokens": "node style-dictionary/build.cjs && npx prettier --write --plugin-search-dir=. ./src/lib/tokens.js ./src/style/_tokens.scss ./src/style/_sass-tokens.scss",
    "update:data": "npm run update:gdoc; npm run update:gsheet",
    "docs": "start-storybook --docs -p 6006 -s ./static --no-open",
    "docs:build": "build-storybook --docs -o docs-build -s ./static",
    "docs:deploy": "scripts/deploy-docs.sh --build",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
  },
  "dependencies": {
    "colorjs.io": "^0.3.0",
    "d3-array": "^3.1.1",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.1.0",
    "fuzzysort": "^1.1.4",
    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@mdx-js/react": "^1.6.22",
    "@rollup/plugin-dsv": "^2.0.3",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-svelte-csf": "^2.0.4",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/svelte": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/adapter-static": "1.0.0-next.46",
    "@sveltejs/kit": "1.0.0-next.525",
    "@types/d3-array": "^3.0.2",
    "@types/d3-dsv": "^3.0.0",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-shape": "^3.1.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/sanitize-html": "^2.5.0",
    "archieml": "^0.4.2",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.5",
    "d3-dsv": "^3.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "googleapis": "^100.0.0",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.4.1",
    "prettier-plugin-jsdoc": "^0.3.30",
    "prettier-plugin-svelte": "^2.5.0",
    "sanitize-html": "^2.5.3",
    "sass": "^1.49.11",
    "style-dictionary": "^3.0.3",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.5",
    "svelte2tsx": "^0.4.10",
    "vite": "^3.1.4"
  },
  "overrides": {
    "node-forge": "^1.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=16.14"
  }
}
